    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-guard": "1.0.3",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.0.1",
    "extract-loader": "5.1.0",
    "file-loader": "5.0.2",
