{
  "name": "personal-page",
  "version": "1.0.0",
  "description": "personal web page",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --verbose --colors",
    "coverage": "npm test -- --coverage",
    "serve": "serve -s ./dist -l 4449",
    "pre-commit": "run-s \"test\" \"clean\" \"lint\"",
    "pre-push": "run-s \"test\" \"clean\" \"lint\"",
    "dev": "run-s \"lint\" \"dev:server\"",
    "dev:dashboard": "run-s \"lint\" \"dev:server:dashboard\"",
    "dev:server": "webpack-dev-server --mode development -d --hot --config ./config/webpack/webpack.dev.js --watch",
    "dev:server:dashboard": "webpack-dashboard -- webpack-dev-server --mode development -d --hot --config ./config/webpack/webpack.dev.js --watch --open",
    "build": "run-s \"lint\" \"clean\" \"build:webpack -- {@}\" \"build:gulp:postbuild -- {@}\" --",
    "build:webpack": "webpack --mode production --config ./config/webpack/webpack.prod.js",
    "build:gulp:postbuild": "gulp postbuild",
    "reset": "rm -rf dist && rm -rf node_modules && rm package-lock.json",
    "clean": "rm -rf dist",
    "lint": "run-s \"lint:js\" \"lint:pcss\"",
    "lint:pcss": "stylelint './src/styles/**/*.pcss'",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:js:fix": "npm run lint:eslint:fix -- . ",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "doc": "jsdoc -c config/jsdoc/conf.json"
  },
  "keywords": [
    "pixi"
  ],
  "author": "iivanovw7",
  "license": "ISC",
  "dependencies": {
    "@pixi/filter-drop-shadow": "3.1.1",
    "core-js": "3.6.5",
    "immer": "7.0.9",
    "pixi.js": "5.2.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/node": "7.7.4",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.7.4",
    "@babel/preset-modules": "0.1.1",
    "autoprefixer": "9.7.2",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-polyfill": "6.26.0",
    "chalk": "3.0.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.5",
