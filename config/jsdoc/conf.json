{
  "source": {
    "include": ["src", "README.md"]
  },
  "opts": {
    "destination": "./dist/doc",
    "template": "node_modules/tui-jsdoc-template",
    "encoding": "utf8",
    "recurse": true,
    "tutorials": "doc"
  },
  "plugins": ["plugins/markdown"],
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "name": "personal-page"
  }
}
